<div class="content_wrapper">
  <table width="95%" align="center" cellspacing="0" cellpadding="4">
    <tr class="tbl_header">
      <td width="40%">
        Course
      </td>
      <td>
        Created on
      </td>
    </tr>
    <% @courses.each do |c| %>
    <tr class="<%= cycle("even", "odd") %>">
      <td>
        <%=c.name %>
      </td>
      <td>
        <%=c.created_at %>
      </td>
    </tr>
    <% end %>
  </table>
</div>