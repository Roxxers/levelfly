<div class="GB_course_box">

  <div class="spacebarGbook"></div>
  <div class="GBtable_wrapper">
    <div class="outcome_wp">
      <div class="Grade_left_panel_head"><div class="Grade_box">GRADE</div></div>
      <div class="Grade_right_panel_head">
        <div class="selectbg2"> <span class="fakebox">ALL OUTCOMES</span>
          <select id="outcome_id">
            <% unless @outcomes.nil? %>
                <% @outcomes.each do |o| %>
                    <option value="<%= o.id %>"><%= o.name %></option>
                <% end %>
            <% end %>
          </select>
        </div>
      </div>
    </div><!--outcome_wp-->

    <div class="Grade_left_panel">
      <table width="100%" border="0" cellpadding="0" cellspacing="0" class="GB_left_table">

        <%if !@participant.nil?%>
        <% @participant.each do |p|%>
        <tr class="<%= cycle("GB_odd","")%>">
          <td width="63%" class="Name_box"><%= p.full_name%></td>
          <td width="37%" align="center" valign="middle"> <div class="b_box">B</div></td>
        </tr>
        <%end%>
        <%end%>
      </table>
    </div><!--Grade_left_panel-->

    <div class="Grade_right_panel">

      <table width="100%" border="0" cellpadding="0" cellspacing="0" class="GB_right_table">
        <%if !@participant.nil?%>
        <% @participant.each do |p|%>
        <tr class="<%= cycle("Dark_no","")%>">
          <td>
            <div class="GB_outcomment rotate_text">WRITING USING A QUILL</div>
          </td>
          <td>1</td>
          <td>2</td>
          <td>2</td>
          <td>1</td>
          <td>2</td>
          <td>&nbsp;</td>
          <td>&nbsp;</td>
          <td>&nbsp;</td>
          <td>&nbsp;</td>
        </tr>
        <%end%>
        <%end%>

      </table>
    </div><!--Grade_right_panel-->
  </div>
</div>

