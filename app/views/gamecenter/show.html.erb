          <img src="../images/Achievements.png" class="Achievements">
        </div>
        <div class="col-sm-8">
           <h3 class="text-info" >Learning Objectives</h3>
           <ul>
             <% @outcomes.each do |outcome|%>
               <li class="learning_text"><%= outcome.name %></li>
             <% end %>
           </ul>  
        </div>    
      </div>
    </div>
    <div class="row">
      <div id="Rectangle6">
