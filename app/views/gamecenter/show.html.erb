          </div>
          <div class="col-sm-8">
            <div class="game_detail">
              <div>
                <h2 class="text-info game_name"><%= @game.name%></h2>
              </div>
              <div> 
                <h4 class="text-info"><b> Description</b></h4>
              </div>  
              <h4>
                <p class="text-info game_desc"><%= @game.descr%></p>
              </h4>
            </div>  
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div id="Stripes_0">
        <div class="screenshot">
          <b> <h4 class="text-info">Screenshots</h4></b>
        </div>
        <div id="thumbs">
