<div class="row show_page">
  <div class="col-md-1">
  </div>
  <div class="col-md-10">
    <div class="row">
      <div id= "Stripes">
    	</div>
    </div>
    <div class="row">
      <div id="section_one">
        <div class="row">
          <div class="col-sm-4">
