<div class="LB_tableheader">
  <div class="LBT_ava"> &nbsp;</div>
  <div class="LBT_name">&nbsp;</div>
  <div class="LBT_level">LEVEL</div>
  <div class="LBT_xp">XP</div>
  <div class="LBT_badges">BADGES</div>
  <div class="LBT_Likes">LIKES</div>
</div>
<% unless @courses_list.nil? %>
  <% @courses_list.each do |c| %> 
    <div class="LB_tablecontent <%= cycle("even", "odd") %>">
      <div class="LBT_ava">
        <div class="avatarWbdr"><img src="<%= c.image_file_name%>" /></div>
      </div>
      <div class="LBT_name"> <span class="name_txt"><%= c.full_name %> </span><br />
        <span class="course_txt"><%= c.name %></span> </div>
      <div class="LBT_level"><%= c.level %></div>
      <div class="LBT_xp"><%= c.points %></div>
      <div class="LBT_badges">17</div>
      <div class="LBT_Likes">943</div>
    </div>
  <% end %>
<% end %>  