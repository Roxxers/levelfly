<div class="LB_tableheader">
  <div class="LBT_rank">Rank</div>
  <div class="LBT_ava"> &nbsp;</div>
  <div class="LBT_name">&nbsp;</div>
  <div class="LBT_level">LEVEL</div>
  <div class="LBT_xp">XP</div>
  <div class="LBT_badges">BADGES</div>
  <div class="LBT_Likes">LIKES</div>
</div>
<% unless @profiles.nil? %>
  <% @profiles.each do |p| %> 
    <div class="LB_tablecontent <%= cycle("even", "odd") %>">
      <div class="LBT_rank"><%= p.rank %></div>
      <div class="LBT_ava">
        <div class="avatarWbdr"><img src="<%= is_profile_accessible?(p) ? p.image_file_name : Profile.default_avatar_image %>" class="profile" profile_id="<%= p.id %>" /></div>
      </div>
      <div class="LBT_name"> <span class="name_txt"><%= is_profile_accessible?(p) ? p.full_name : 'Private'%> </span><br />
      <span class="course_txt"><%= is_profile_accessible?(p) ? p.major_school : ''%></span> </div>
      <div class="LBT_level"><%= is_profile_accessible?(p) ? p.level : 'Private'%></div>
      <div class="LBT_xp"><%= is_profile_accessible?(p) ? p.xp : 'Private' %></div>
      <div class="LBT_badges"><%= is_profile_accessible?(p) ? p.badge_count : 'Private'%></div>
      <div class="LBT_Likes"><%= is_profile_accessible?(p) ? p.like_received : 'Private'%></div>      
    </div>
  <% end %>
<% end %>
